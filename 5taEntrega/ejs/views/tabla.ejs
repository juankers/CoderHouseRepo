<%- include('./encabezado')-%>
<div class="container p-4">
        <h1>Listado productos</h1>
        <table class="table">
            <tr style="color: black;">
                <th>Title</th>
                <th>Price</th>
                <th>Thumbnail</th>
            </tr>
            <%productos.forEach(function(producto) {%>
                <tr>
                    <td>
                        <%=producto.nombre%>
                    </td>
                    <td>
                        <%=producto.price%>
                    </td>
                    <td>
                        <%=producto.thumbnail%>
                    </td>
                </tr>
                <%});%>
        </table>
        <a href="http://localhost:8080/" class="btn btn-primary">Nuevo producto</a>
    </div>